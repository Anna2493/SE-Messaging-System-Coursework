        <ListBox x:Name="lstMentions" HorizontalAlignment="Left" Height="141" Margin="217,71,0,0" VerticalAlignment="Top" Width="147" SelectionChanged="lstMentions_SelectionChanged"/>

    </Grid>
</Window>
